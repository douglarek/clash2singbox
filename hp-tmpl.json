{
  "log": {
    "level": "warn",
    "output": "/var/run/homeproxy/sing-box-c.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "cf",
        "address": "tls://1.1.1.1"
      },
      {
        "tag": "local",
        "address": "223.5.5.5",
        "detour": "direct-out"
      },
      {
        "tag": "block-dns",
        "address": "rcode://name_error"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "local"
      }
    ],
    "final": "cf"
  },
  "inbounds": [
    {
      "type": "direct",
      "tag": "dns-in",
      "listen": "::",
      "listen_port": 5333
    },
    {
      "type": "redirect",
      "tag": "redirect-in",
      "listen": "::",
      "listen_port": 5331,
      "sniff": true,
      "sniff_override_destination": true
    },
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 5332,
      "sniff": true,
      "sniff_override_destination": true,
      "network": "udp"
    }
  ],
  "route": {
    "geoip": {
      "path": "/var/run/homeproxy/geoip.db",
      "download_detour": "select"
    },
    "geosite": {
      "path": "/var/run/homeproxy/geosite.db",
      "download_detour": "select"
    },
    "rules": [
      {
        "inbound": "dns-in",
        "outbound": "dns-out"
      },
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct-out"
      },
      {
        "geosite": [
          "cn",
          "private"
        ],
        "geoip": [
          "cn",
          "private"
        ],
        "outbound": "direct-out"
      }
    ],
    "final": "select",
    "auto_detect_interface": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/etc/homeproxy/resources/ui",
      "store_selected": true
    }
  }
}

